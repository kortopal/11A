{
    "name": "11/A Akıllı Tahta Eklentisi",
    "description": "Çok amaçlı eklenti... Geliştirilmeye başlanma tarihi: 13.01.2022",
    "version": "2.1",
    "manifest_version": 3,
    "background.service_worker": {
      "scripts": ["background.js"],
      "persistent": false
      },

    "permissions": [
      "storage"
      ],
      
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
      },

    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/16.png",
          "32": "/images/32.png",
          "48": "/images/48.png",
          "128": "/images/128.png"
        }
      }
}
