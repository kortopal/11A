{
    "name": "11/A Akıllı Tahta Eklentisi",
    "description": "Çok amaçlı eklenti... Geliştirilmeye başlanma tarihi: 13/01/2022",
    "version": "3.0",
    "manifest_version": 3,
    "background.service_worker": {
      "scripts": ["/js/background.js"],
      "persistent": false
      },

    "permissions": [
      "storage",
      "unlimitedStorage",
      "tabs"
      ],
      
    "icons": {
        "16": "/img/16.png",
        "32": "/img/32.png",
        "48": "/img/48.png",
        "128": "/img/128.png"
      },

    "action": {
        "default_popup": "popup.html",
        "default_icon": "/img/48.png"
      }
}